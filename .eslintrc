{
	"extends": ["airbnb", "prettier/react"],
	"plugins": ["prettier"],
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 2016,
		"sourceType": "module"
	},
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
	"rules": {
		"react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
		"no-underscore-dangle": [1, { "allow": ["_id"] }],
		"strict": 0,
		"quotes": 0,
		"no-unused-vars": 0,
		"camelcase": 0
}
}
